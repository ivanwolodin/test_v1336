#include "widget.h"
#include "ui_widget.h"

Widget::Widget(QWidget *parent) :
    QWidget(parent),
    ui(new Ui::Widget)
{
    ui->setupUi(this);
    connect(ui->processed_button,  // who is calling signal
            SIGNAL(clicked(bool)), // signal itself
            this,                  // who udergoes
            SLOT(close()));        // what to do
}

Widget::~Widget()
{
    delete ui;
}
